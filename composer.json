{
  "name": "npabisz/laravel-settings",
  "description": "Settings for Laravel.",
  "keywords": ["laravel", "settings"],
  "license": "MIT",
  "authors": [
    {
      "name": "Norbert Pabisz",
      "email": "yorki.ogame@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "illuminate/contracts": "^9.0",
    "illuminate/database": "^9.0",
    "illuminate/support": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Npabisz\\LaravelSettings\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Npabisz\\LaravelSettings\\SettingsServiceProvider"
      ],
      "aliases": {
        "Settings": "Npabisz\\LaravelSettings\\Facades\\Settings"
      }
    }
  },
  "prefer-stable": true
}